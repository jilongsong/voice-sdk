{
  "name": "web-voice-kit",
  "version": "0.1.1",
  "description": "A lightweight browser voice SDK with wake word and transcription, built with Vite.",
  "type": "module",
  "main": "dist/voice-sdk.cjs.js",
  "module": "dist/voice-sdk.es.js",
  "exports": {
    ".": {
      "import": "./dist/voice-sdk.es.js",
      "require": "./dist/voice-sdk.cjs.js"
    }
  },
  "unpkg": "dist/voice-sdk.umd.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.types.json",
    "preview": "vite preview",
    "clean": "rimraf dist"
  },
  "keywords": [
    "voice",
    "sdk",
    "speech-to-text",
    "wake word",
    "transcription",
    "vite"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "crypto-js": "^4.2.0",
    "vosk-browser": "^0.0.5"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/dom-speech-recognition": "^0.0.7",
    "@types/node": "^20.11.30",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.0",
    "vite": "^5.0.0"
  }
}
